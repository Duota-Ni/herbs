.nav{
  position: fixed;
  top: 30rpx;
  right: 60rpx;
  width: 50rpx;
  height: 50rpx;
}
.box{
  width: 720rpx;
  margin: 10rpx;
  padding: 10rpx;
  border: solid 1px lightgrey;
  overflow-wrap: break-word;
}
.name{
  font-size: 40rpx;
  color: rgb(255, 153, 0);
  padding: 10rpx;
  margin: 30rpx;
  text-align: center;
}
.actionSheetItemsout{
  padding: 10px;
  margin: 10px;
}
.actionSheetItemsin{
  margin-top: 0;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
}
action-sheet-item{
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 14px;
  width: 20%;
  border: solid #69cfff 1px;
  border-radius: 15rpx;
  margin: 5px;
}
page {
  height: 100%;
}
.jump_list{
  position: relative;
  height: 100%;
}
/*滚动条*/
::-webkit-scrollbar
{
width: 24rpx;
height: 24rpx;
background-color: #fff;
}
::-webkit-scrollbar-track
{
background-color: #69d0ff27;
}
::-webkit-scrollbar-thumb
{
background-color: #69cfff;
} 
 
 
