.container{
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.protectnamebox{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  margin: 10rpx;
  padding: 10rpx;
  border: solid 1px lightgrey;
  overflow-wrap: break-word;
}
.protectname{
  flex:0 0 40%;
  justify-content: center;
  align-items: center;
  font-size: 13px;
  text-align: center;
  margin:5px;
  padding: 4px 5px 4px 5px;
  background-color: rgba(250, 224, 199, 0.788);
  border-radius: 20rpx;
}
.h1{
  font-size: 20px;
  color:#095a42;
  padding: 5px;
}
